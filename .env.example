# Intent-Driven Cold Outreach Agent - Environment Configuration Template
# Copy this file to .env.preprod and fill in your actual values

# Application Configuration
PORT=3000
NODE_ENV=preprod

# API Keys (if needed for future integrations)
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Masumi Network Configuration
MASUMI_NETWORK=preprod

# Logging Configuration
LOG_LEVEL=info
ENABLE_VERBOSE_LOGGING=false

# Processing Configuration
PROCESSING_TIMEOUT=30000
MAX_REVISION_ATTEMPTS=3

# Testing Configuration (for test environments)
TEST_TIMEOUT=60000
PROPERTY_TEST_ITERATIONS=100

# Security Configuration
CORS_ORIGIN=*
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Performance Configuration
MAX_CONCURRENT_REQUESTS=10
CACHE_TTL_SECONDS=300

# Monitoring Configuration (optional)
# SENTRY_DSN=your_sentry_dsn_here
# NEW_RELIC_LICENSE_KEY=your_new_relic_key_here