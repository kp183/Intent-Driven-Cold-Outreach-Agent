# Intent-Driven Cold Outreach Agent - Pre-Production Environment
# This file contains the actual configuration for pre-production deployment

# Application Configuration
PORT=3000
NODE_ENV=preprod

# API Keys (currently not needed - system is self-contained)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=

# Masumi Network Configuration
MASUMI_NETWORK=preprod

# Logging Configuration
LOG_LEVEL=info
ENABLE_VERBOSE_LOGGING=false

# Processing Configuration
PROCESSING_TIMEOUT=30000
MAX_REVISION_ATTEMPTS=3

# Testing Configuration
TEST_TIMEOUT=60000
PROPERTY_TEST_ITERATIONS=100

# Security Configuration
CORS_ORIGIN=*
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Performance Configuration
MAX_CONCURRENT_REQUESTS=10
CACHE_TTL_SECONDS=300

# Monitoring Configuration
# SENTRY_DSN=
# NEW_RELIC_LICENSE_KEY=